{
  "name": "mint-nft",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:css && vite build",
    "build:css": "postcss styles/tailwind.css -o src/assets/styles.css",
    "build:vite": "vite build",
    "deploy": "gh-pages -d dist",
    "dev": "run-p dev:*",
    "dev:css": "postcss styles/tailwind.css -o src/assets/styles.css --watch",
    "dev:vite": "vite",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint . --fix",
    "predeploy": "$npm_execpath run build",
    "preview": "vite preview",
    "start": "$npm_execpath run dev",
    "typecheck": "tsc --noEmit",
    "validate": "run-p lint typecheck"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@rainbow-me/rainbowkit": "^2.1.3",
    "@tanstack/react-query": "^5.51.18",
    "@wagmi/chains": "^1.8.0",
    "@wagmi/core": "^2.12.2",
    "@web3-react/core": "^6.1.9",
    "clsx": "^2.1.0",
    "ethers": "^6.13.2",
    "framer-motion": "^11.3.24",
    "gh-pages": "^6.1.1",
    "next": "^14.2.5",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.74.4",
    "react-router-dom": "^6.22.3",
    "scrollreveal": "^4.0.9",
    "tailwind-merge": "^2.2.2",
    "tiny-invariant": "^1.3.3",
    "viem": "^2.18.7",
    "wagmi": "^2.12.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/scrollreveal": "^0.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  }
}
